#include <stdio.h>
#include <stdlib.h>
#define x 10
int busca(int v[], int n, int chave);

int main(int argc, char *argv[]) {
	
	int v[x] = {2, 4, 10, 23, 12, 2, 4, 8, 9, 15};
	int i;
	
	return 0;
}

int busca(int v[], int n, int chave){
	int i;
	for(i=0;i<n;i++){
		if(v[i] == chave){
			printf("Valor: %i", v[i]);
			return i;
		}else{
			printf("Erro na parada!!!");
		}
	 return -1;
	}
}
