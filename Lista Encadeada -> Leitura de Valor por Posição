int leitura_pos(){
  //Validação!!!
  if( lista_vazia(*p))
    return ERRO_LISTA_VAZIA;
  if(pos < 0 || pos > p->qtd)
    return ERRO_POS_INVALIDA;

  //Alocar -> Percorrer -> Copiar!!!
  Elemento *aux = p->cabeca;
  int cont;
  for(cont = 0;cont < pos;cont++) //Percorre a Lista!!!
    aux = aux->proximo;
  memcpy(info, aux->info,l.tamInfo); //Faz a copia do elemento!!!
  return 1;
{
